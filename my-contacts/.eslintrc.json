{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "arrow-parens":"off",
    "import/extensions": "off",
    "no-promise-executor-return": "off",
    "object-curly-newline":"off"
  }
}
